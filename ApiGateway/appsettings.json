{
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "orders",
        "ClusterId": "ordersCluster",
        "Match": {
          "Path": "/orders/{**catch-all}"
        }
      },
      {
        "RouteId": "payments",
        "ClusterId": "paymentsCluster",
        "Match": {
          "Path": "/payments/{**catch-all}"
        }
      }
    ],
    "Clusters": {
      "ordersCluster": {
        "Destinations": {
          "ordersApi": {
            "Address": "http://ordersservice:80/"
          }
        }
      },
      "paymentsCluster": {
        "Destinations": {
          "paymentsApi": {
            "Address": "http://paymentsservice:80/"
          }
        }
      }
    }
  }
}
